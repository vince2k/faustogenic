<%# app/views/dishes/create.turbo_stream.erb %>
<%= turbo_stream.append "meal_#{@meal.id}_dishes" do %>
  <%= render partial: "dishes/table", locals: { dish: @dish } %>
<% end %>

<%= turbo_stream.replace "meal_#{@meal.id}_add_dish" do %>
  <%= render partial: "meals/form", locals: { meal: @meal } %>
<% end %>

